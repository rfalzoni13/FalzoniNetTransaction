{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "Logger",
      "FormatterOptions": {
        "TimestampFormat": "[dd/MM/yyyy HH:mm:ss] ",
        "UseUtcTimestamp": false
      }
    }
  },
  "AllowedHosts": "*"
}
